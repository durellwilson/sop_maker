{
  "name": "sop_maker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:ci": "start-server-and-test dev http://localhost:3000 cypress",
    "test:e2e:smoke": "start-server-and-test dev http://localhost:3000 \"cypress run --spec cypress/e2e/smoke.cy.ts\"",
    "test:e2e:retry": "start-server-and-test dev http://localhost:3000 \"cypress run --config retries=5\"",
    "test:e2e:debug": "DEBUG=cypress:* start-server-and-test dev http://localhost:3000 \"cypress open --config-file cypress.debug.config.ts\"",
    "test:e2e:alt-port": "next dev -p 3333 & wait-on http://localhost:3333 && cypress run --config baseUrl=http://localhost:3333 --spec \"cypress/e2e/smoke.cy.ts\" ; kill $!",
    "test:simple": "next dev -p 4000 & sleep 10 && cypress run --config baseUrl=http://localhost:4000 --spec \"cypress/e2e/smoke.cy.ts\" ; kill $!",
    "cypress": "cypress run",
    "cypress:verify": "cypress verify",
    "cypress:clear": "cypress cache clear && rimraf .cypress/cache",
    "debug": "node scripts/debug.js",
    "debug:test": "node --inspect=9230 node_modules/.bin/jest --runInBand",
    "debug:e2e": "CYPRESS_DEBUG=true cypress open",
    "debug:analyze": "ANALYZE=true next build",
    "test:minimal": "next dev -p 5000 & sleep 5 && cypress run --config baseUrl=http://localhost:5000 --spec \"cypress/e2e/minimal.cy.ts\" ; kill $!",
    "test:static": "next dev -p 6000 & sleep 5 && cypress run --config baseUrl=http://localhost:6000 --spec \"cypress/e2e/static.cy.ts\" ; kill $!",
    "test:static-server": "npx serve ./public -p 7000 & sleep 2 && cypress run --config baseUrl=http://localhost:7000 --spec \"cypress/e2e/static.cy.ts\" ; kill $!",
    "cypress:full-debug": "CYPRESS_DEBUG=1 DEBUG=cypress:*,socket.io:* npx cypress run --config-file cypress.debug.config.ts --spec \"cypress/e2e/minimal.cy.ts\" --browser chrome --no-exit",
    "db:migrate": "ts-node src/scripts/apply-migrations.ts",
    "diagnostic": "node scripts/diagnostic.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/lib-storage": "^3.782.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^5.0.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^5.1.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "firebase": "^11.6.0",
    "firebase-admin": "^13.2.0",
    "lucide-react": "^0.487.0",
    "next": "^15.3.0",
    "openai": "^4.93.0",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-textarea-autosize": "^8.5.9",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/pg": "^8.11.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.4.1",
    "cypress": "^14.3.1",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
